{
  "name": "travel-agency",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "node server-wrapper.js",
    "typecheck": "react-router typegen && tsc",
    "build:backend": "cd backend && npm run build",
    "start:backend": "cd backend && npm start",
    "build:all": "npm run build && npm run build:backend",
    "start:prod": "concurrently \"npm run start:backend\" \"npm start\"",
    "preview": "npm run build && npm start",
    "deploy": "pm2 start ecosystem.config.js",
    "deploy:stop": "pm2 stop ecosystem.config.js",
    "deploy:restart": "pm2 restart ecosystem.config.js",
    "deploy:reload": "pm2 reload ecosystem.config.js",
    "deploy:logs": "pm2 logs"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@react-router/express": "^7.5.3",
    "@react-router/node": "^7.5.3",
    "@react-router/serve": "^7.5.3",
    "@syncfusion/ej2-base": "^29.2.4",
    "@syncfusion/ej2-react-base": "^29.2.4",
    "@syncfusion/ej2-react-buttons": "^29.2.5",
    "@syncfusion/ej2-react-charts": "^29.2.5",
    "@syncfusion/ej2-react-dropdowns": "^29.2.5",
    "@syncfusion/ej2-react-grids": "^29.2.5",
    "@syncfusion/ej2-react-maps": "^29.2.4",
    "@syncfusion/ej2-react-navigations": "^29.2.5",
    "@syncfusion/ej2-react-splitbuttons": "^29.2.4",
    "appwrite": "^18.1.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "isbot": "^5.1.27",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.3",
    "react-router-dom": "^7.7.0",
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@react-router/dev": "^7.5.3",
    "@tailwindcss/vite": "^4.1.4",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/react-router-dom": "^5.3.3",
    "concurrently": "^9.2.0",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
